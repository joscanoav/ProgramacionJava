package main;

public class PrimerPrograma {

	public static void main(String[] args) {
		// Write()
		//No añade salto
		System.out.print("Hola mundo");
		//Añade salto
		System.out.println("Adios mundo");

		int edad = 40;
		float altura = 1.85f;
		
		
		System.out.println("Tiene " + edad + " años y mide " + altura + " metros" );
		
		// int x = 4
		// int y = 6
		int x = 4, y = 6;
		
		System.out.println("La suma de " + x + "con " + y + "es igual a  " + (x + y) );
		
		
	}
}
